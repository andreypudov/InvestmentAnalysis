<?xml version="1.0" encoding="windows-1251"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="REPORT_DOC">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="DOC_REQUISITES">
                    <xs:complexType>
                        <xs:annotation>
                            <xs:documentation>Реквизиты документа</xs:documentation>
                        </xs:annotation>
                        <xs:sequence>
                            <xs:element name="TEMPLATE">
                                <xs:complexType>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>служебная информация</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Title" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>Заголовок документа</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Text" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>заголовок отчета</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Firm" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>наименование брокера</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DateBegin" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>дата начала отчета</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DateEnd" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>дата окончания отчета</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DateCreate" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>Дата создания отчета</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountDays" type="xs:string" use="required" >
                            <xs:annotation>
                                <xs:documentation>количество дней, за которые построен отчет</xs:documentation>
                            </xs:annotation>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>
                <xs:element name="HEADER_DOC">
                    <xs:complexType>
                        <xs:annotation>
                            <xs:documentation>Заголовок документа</xs:documentation>
                        </xs:annotation>
                        <xs:sequence>
                            <xs:element name="ACCOUNT">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>Счет клиента</xs:documentation>
                                    </xs:annotation>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>наименование счета клиента</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ID" type="xs:unsignedInt" use="required" />
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="CLIENT">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>Клиент</xs:documentation>
                                    </xs:annotation>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>наименование клиента</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="CONTRACT">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>Договор с клиентом</xs:documentation>
                                    </xs:annotation>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>наименование договора</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="DP"  nillable="true" >
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>Счет(а) депо</xs:documentation>
                                    </xs:annotation>
                                    <xs:attribute name="N" type="xs:string" use="optional" >
                                        <xs:annotation>
                                            <xs:documentation>наименование</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="E">
                                <xs:complexType>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>не используется</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="H">
                                <xs:complexType>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>не используется</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="SECTIONS">
                    <xs:complexType>
                        <xs:annotation>
                            <xs:documentation>разделы "Отчета брокера"</xs:documentation>
                        </xs:annotation>
                        <xs:sequence>
                            <xs:element name="DB1">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>таблица "Оценка состояния счета Клиента"</xs:documentation>
                                    </xs:annotation>
                                    <xs:sequence>
                                        <xs:element name="T">
                                            <xs:complexType>
                                                <xs:annotation>
                                                    <xs:documentation>строка с итогами таблицы "Оценка состояния счета Клиента"</xs:documentation>
                                                </xs:annotation>
                                                <xs:attribute name="Name" type="xs:string" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>итого</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="B" type="xs:decimal" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>итоги на начало периода</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="E" type="xs:decimal" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>итоги на конец периода</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="Delta" type="xs:decimal" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>изменение за период</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element maxOccurs="unbounded" name="R">
                                            <xs:complexType>
                                                <xs:annotation>
                                                    <xs:documentation>строка таблицы "Оценка состояния счета Клиента"</xs:documentation>
                                                </xs:annotation>
                                                <xs:attribute name="Name" type="xs:string" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>наименование параметра</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="B" type="xs:decimal" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>значение на начало периода</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="E" type="xs:decimal" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>значение на конец периода</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>Название таблицы</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="DB2">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>таблица "Оценка рублевой позиции"</xs:documentation>
                                    </xs:annotation>
                                    <xs:sequence>
                                        <xs:element minOccurs="0" name="R">
                                            <xs:complexType>
                                                <xs:annotation>
                                                    <xs:documentation>строка таблицы "Оценка рублевой позиции"</xs:documentation>
                                                </xs:annotation>
                                                <xs:sequence>
                                                    <xs:element name="B">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>раздел "На начало периода" таблицы "Оценка рублевой позиции"</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Оценка входящей денежной позиции</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Free" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Свободные</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Blk" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Блокировано</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Debt" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Задолженность</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Tax" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Задолженность  по налогам (справочно)*</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="E">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>раздел "На конец периода" таблицы "Оценка рублевой позиции"</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Оценка исходящей денежной позиции (руб)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Free" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Свободный исходящий остаток</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Blk" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Блокировано</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Debt" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Задолженность</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Tax" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Задолженность  по налогам (справочно)*</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="Name" type="xs:string" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>Валюта</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>Название таблицы</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="DB20">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>таблица "Оценка денежной позиции"</xs:documentation>
                                    </xs:annotation>
                                    <xs:sequence>
                                        <xs:element minOccurs="0" name="R">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="B">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>На начало периода</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="SumC" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Входящий плановый остаток</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Free" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                <xs:documentation>в т.ч. Свободный входящий плановый остаток</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Blk" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Блокировано</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Debt" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Задолженность</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Tax" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Задолженность  по налогам (справочно)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Оценка денежной позиции (руб)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="E">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>На конец периода</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="SumC" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Исходящий остаток</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Free" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                <xs:documentation>в т.ч. Свободный исходящий плановый остаток</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Blk" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Блокировано</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Mrg" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Задолженность маржинальная</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Debt" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>в т.ч. Задолженность прочая</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Tax" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Задолженность  по налогам (справочно)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="required" >
                                                                <xs:annotation>
                                                                    <xs:documentation>Оценка денежной позиции (руб)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="E">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>Плановый остаток</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="T1" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>на Т+1</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="T2" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                <xs:documentation>на Т+2</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="T3" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>на Т+3 </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="TN" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>на Т+n</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="Name" type="xs:string" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>Валюта</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="T">
                                            <xs:complexType>
                                                <xs:annotation>
                                                    <xs:documentation>строка с итогами таблицы "Оценка денежной позиции"</xs:documentation>
                                                </xs:annotation>
                                                <xs:sequence>
                                                    <xs:element name="B">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>итоги, раздел "На начало периода" таблицы "Оценка денежной позиции"</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>сумма по колонке На начало периода Оценка  денежной позиции (руб)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="E">
                                                        <xs:complexType>
                                                            <xs:annotation>
                                                                <xs:documentation>итоги, раздел "На конец периода" таблицы "Оценка денежной позиции"</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:attribute name="Sum" type="xs:decimal" use="optional" >
                                                                <xs:annotation>
                                                                    <xs:documentation>сумма по колонке На конец периода Оценка денежной позиции (руб)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Name" type="xs:string" use="required" >
                                        <xs:annotation>
                                            <xs:documentation>Название таблицы</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="DB21">
                                <xs:complexType>
                                    <xs:annotation>
                                        <xs:documentation>таблица "Виды движений денежный средств"</xs:documentation>
                                    </xs:annotation>
                                    <xs:sequence>
                                        <xs:element minOccurs="0" maxOccurs="unbounded" name="R">
                                            <xs:complexType>
                                                <xs:annotation>
                                                    <xs:documentation>строка таблицы Виды движений денежный средств</xs:documentation>
                                                </xs:annotation>
                                                <xs:attribute name="Name" type="xs:string" use="required" >
                                                    <xs:annotation>
                                                        <xs:documentation>Валюта</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="MI" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Ввод ДС</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="MO" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Вывод ДС</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="SNE" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Получено от продажи - внебиржевые сделки</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="SE" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Получено от продажи - биржевые сделки с ЦБ</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="SV" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Получено от продажи - биржевые сделки с валютой</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="BNE" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Затрачено на покупку - внебиржевые сделки</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="BE" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Затрачено на покупку - биржевые сделки с ЦБ</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="BV" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Затрачено на покупку - биржевые сделки с валютой</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="VM" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Вариационная маржа / Премия</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="GO" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Изменение ГО</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="C" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Биржевой сбор</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="D" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Дивиденды / НКД</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="BC" type="xs:decimal" use="optional" >
                                                    <xs:annotation>
                                                        <xs:documentation>Вознаграждение брокера</xs:documentation>
